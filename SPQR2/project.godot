; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "EnemyAI",
"language": "GDScript",
"path": "res://scripts/enemy.gd"
}, {
"base": "Node",
"class": "Unit",
"language": "GDScript",
"path": "res://scripts/unit.gd"
} ]
_global_script_class_icons={
"EnemyAI": "",
"Unit": ""
}

[application]

config/name="SPQR"
run/main_scene="res://scenes/world/World.tscn"
config/icon="res://icon.png"

[autoload]

cn="*res://scripts/constants.gd"
helpers="*res://scripts/helpers.gd"
enemy="*res://scripts/enemy.gd"
unit="*res://scripts/unit.gd"
graph="*res://scripts/graph.gd"
data="*res://scripts/data.gd"
config="*res://scripts/config.gd"

[display]

window/size/width=1400
window/size/height=800
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/zylann.project_stats/plugin.cfg" )

[global]

hdr=false

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": true,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

